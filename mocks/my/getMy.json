{
  "expression": "/graphql",
  "body": "query(.*)getMy",
  "method": "POST",
  "name": "my",
  "responses": {
    "rector": {
      "default": true,
      "status": 200,
      "delay": 200,
      "file": "mocks/my/data/rector.json"
    },
    "getUnauthenticated": {
      "status": 200,
      "delay": 200,
      "file": "mocks/my/data/unauthenticated.json"
    }
  }
}
